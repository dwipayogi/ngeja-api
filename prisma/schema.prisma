generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id       String   @id @default(cuid())
  username String   @unique
  email    String   @unique
  number   String   @unique
  exp      Int      @default(0)
  image    String?
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}